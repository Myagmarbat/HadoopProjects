hadoop fs -mkdir /user/cloudera 
hadoop fs -mkdir /user/cloudera/stripes 
hadoop fs -mkdir /user/cloudera/stripes/input
echo "B12 C31 D76 A12 B76 B12 D76 C31 A10 B12 D76" > file0
echo "C31 D76 B12 A12 C31 D76 B12 A12 D76 A12 D76" > file1
hadoop fs -put file* /user/cloudera/stripes/input

hadoop fs -rm -r /user/cloudera/stripes/output
hadoop jar stripes.jar stripes.Driver /user/cloudera/stripes/input /user/cloudera/stripes/output

hadoop fs -cat /user/cloudera/stripes/output/*